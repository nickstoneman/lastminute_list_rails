    <title>Creative - Start Bootstrap Theme</title>
    <!-- Bootstrap Core CSS -->
    <%= stylesheet_link_tag 'https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css' %>

    <!-- Custom Fonts -->
    <%= stylesheet_link_tag 'http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800' %>
    <%= stylesheet_link_tag 'http://fonts.googleapis.com/css?family=Merriweather:400,300,300italic,400italic,700,700italic,900,900italic' %>
    <%= stylesheet_link_tag 'https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.4.0/css/font-awesome.min.css' %>

    <!-- Plugin CSS -->
    <%= stylesheet_link_tag "https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.4.0/animate.min.css" %>

    <!-- Custom CSS -->
    <%= stylesheet_link_tag 'creative.css' %>

    <%= stylesheet_link_tag 'https://cdnjs.cloudflare.com/ajax/libs/bootstrap-material-design/0.3.0/fonts/Material-Design-Icons.ttf' %>
